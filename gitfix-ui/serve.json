{
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "**", "destination": "/index.html" }
  ]
}